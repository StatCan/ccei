{#
/**
 * @file indicators_entity.html.twig
 * Default theme implementation to present Indicators data.
 *
 * This template is used when viewing Indicators pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_indicators_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('indicators_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
